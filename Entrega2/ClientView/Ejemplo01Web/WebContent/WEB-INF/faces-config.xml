<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<managed-bean>
		<managed-bean-name>loginUsuariosBean</managed-bean-name>
		<managed-bean-class>controlador.LoginUsuariosBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ingreso</from-outcome>
			<to-view-id>/ingreso.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>adicionarUsuarios</from-outcome>
			<to-view-id>/adicionarUsuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ingreso.xhtml</display-name>
		<from-view-id>/ingreso.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>fracaso</from-outcome>
			<to-view-id>/loginError.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>loginError.xhtml</display-name>
		<from-view-id>/loginError.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reintentar</from-outcome>		
			<to-view-id>/ingreso.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>loginExito.xhtml</display-name>
		<from-view-id>/loginExito.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>aceptar</from-outcome>				
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>adicionarUsuarios.xhtml</display-name>
		<from-view-id>/adicionarUsuarios.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>insertado</from-outcome>		
			<to-view-id>/insertarExito.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>adicionarUsuarios.xhtml</display-name>
		<from-view-id>/adicionarUsuarios.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>existe</from-outcome>		
			<to-view-id>/insertarError.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>insertarExito.xhtml</display-name>
		<from-view-id>/insertarExito.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>aceptar</from-outcome>						
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>insertarError.xhtml</display-name>
		<from-view-id>/insertarError.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reintentar</from-outcome>						
			<to-view-id>/adicionarUsuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listadoUsuarios</from-outcome>
			<to-view-id>/listadoUsuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listadoUsuarios.xhtml</display-name>
		<from-view-id>/listadoUsuarios.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>exito</from-outcome>
			<to-view-id>/listarUsuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>listarUsuarios.xhtml</display-name>
		<from-view-id>/listarUsuarios.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>aceptar</from-outcome>		
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ingreso.xhtml</display-name>
		<from-view-id>/ingreso.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>exito</from-outcome>
			<to-view-id>/loginExito.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>loginExito.xhtml</display-name>
		<from-view-id>/loginExito.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>verEstadisticas</from-outcome>
			<to-view-id>/verEstadisticas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
		<navigation-rule>
		<display-name>loginExito.xhtml</display-name>
		<from-view-id>/loginExito.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>tienda</from-outcome>
			<to-view-id>/tiendaMain.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>tiendaMain.xhtml</display-name>
		<from-view-id>/tiendaMain.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/loginExito.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>verEstadisticas.xhtml</display-name>
		<from-view-id>/verEstadisticas.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/loginExito.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
